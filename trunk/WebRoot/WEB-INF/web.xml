<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>zhuangyuan</display-name>
	<description>zhuangyuan</description>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>StartAdmin</servlet-name>
		<servlet-class>zy.util.StartMe</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>StartAdmin</servlet-name>
		<url-pattern>/StartAdmin</url-pattern>
	</servlet-mapping>

</web-app>